{
  "name": "react-hn",
  "version": "0.0.0",
  "private": true,
  "description": "Hacker News built with React and Flux",
  "main": "src/app.coffee",
  "scripts": {
    "start": "watchify -t coffeeify -o bundle.js -v -d src/app.coffee",
    "sync": "browser-sync start --server --no-open --files bundle.js",
    "test": "watch jest src -d"
  },
  "author": "Boshen",
  "dependencies": {
    "firebase": "^2.2.7",
    "lodash": "^3.9.3",
    "react": "^0.13.3",
    "reactfire": "^0.4.0"
  },
  "devDependencies": {
    "browser-sync": "^2.7.6",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "jest-cli": "^0.4.8",
    "watch": "^0.16.0",
    "watchify": "^3.2.2"
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "unmockedModulePathPatterns": [
      "node_modules/react"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ]
  }
}
