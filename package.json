{
  "name": "react-hn",
  "version": "0.0.0",
  "private": true,
  "description": "Hacker News built with React and Flux",
  "main": "src/app.coffee",
  "scripts": {
    "start": "watchify -t coffeeify -o bundle.js -v -d src/app.coffee",
    "sync": "browser-sync start --server --no-open --no-notify --no-ui --files 'bundle.js, app.css'",
    "test": "watch jest src -d --wait=1"
  },
  "author": "Boshen",
  "dependencies": {
    "firebase": "^2.2.7",
    "flux": "^2.0.3",
    "immutable": "^3.7.3",
    "lodash": "^3.9.3",
    "material-ui": "^0.9.1",
    "q": "^1.4.1",
    "react": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7"
  },
  "devDependencies": {
    "browser-sync": "^2.7.6",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "jest-cli": "^0.4.8",
    "watch": "^0.16.0",
    "watchify": "^3.2.2"
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "unmockedModulePathPatterns": [
      "node_modules"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ]
  }
}
